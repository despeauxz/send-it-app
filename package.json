{
  "name": "send-it-app",
  "version": "1.0.0",
  "description": "A courier delivery web application",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./dist/lib/app.js",
    "start:dev": "nodemon --exec babel-node -- ./server/lib/app.js",
    "clean": "rm -rf dist coverage .nyc_output",
    "build": "mkdir dist && babel server -d dist",
    "test": "mocha ./server/tests/tests.js --require babel-register --recursive --timeout 50000 --exit",
    "test:coverage": "nyc --extension .es6 --reporter=lcov --reporter=text mocha 'server/tests/tests.js'  --compilers babel-register --recursive --timeout 50000 --exit",
    "coveralls": "npm run test:coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/despeauxz/send-it-app.git"
  },
  "keywords": [
    "MEAN",
    "Node",
    "Javascript"
  ],
  "author": "Malik Godwin Onimisi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/despeauxz/send-it-app/issues"
  },
  "homepage": "https://github.com/despeauxz/send-it-app#readme",
  "dependencies": {
    "@babel/core": "^7.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "supertest": "^3.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/supertest": "^2.0.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0",
    "sinon": "^7.1.1"
  }
}
